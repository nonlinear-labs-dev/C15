[Unit]
Description=Nonlinear-Labs Midi over Ethernet service
After=systemd-modules-load.service sound.target
StartLimitIntervalSec=0
StartLimitInterval=0

[Service]
ExecStartPre=modprobe snd_seq
ExecStartPre=modprobe snd_virmidi
ExecStart=/usr/bin/aseqnet
Restart=on-failure
RestartSec=1

[Install]
WantedBy=multi-user.target
