<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:nl='urn:import:com.nonlinearlabs.client.world.overlay.belt.presets.html'>
	<g:HTMLPanel styleName="belt-preset-ui">
		<div class="presetmanager-area">
			<div class="preset-area">
				<g:Label styleName="title">Preset</g:Label>
				<g:Button styleName="show-menu" ui:field="showPresetMenu">
					<img src="images/Menu_Preset_Enabled_S.svg"></img>
				</g:Button>

				<g:Button styleName="show-info" ui:field="showPresetInfo">
					<img src="images/Menu_Info_Enabled_S.svg"></img>
				</g:Button>

				<g:Button styleName="show-search" ui:field="showPresetSearch">
					<img src="images/Menu_Search_Enabled_S.svg"></img>
				</g:Button>
			</div>
			<div class="bank-area">
				<g:Label styleName="title">Bank</g:Label>
				<g:Button styleName="show-menu" ui:field="showBankMenu">
					<img src="images/Menu_Bank_Enabled_S.svg"></img>
				</g:Button>
				<g:Button styleName="show-info" ui:field="showBankInfo">
					<img src="images/Menu_Info_Enabled_S.svg"></img>
				</g:Button>
			</div>
		</div>

		<div class="bank-area">
			<div class="store-area">
				<div class="upper">
					<g:Button styleName="show-menu" ui:field="showStoreMenu">
						<img src="images/Append_Enabled.svg"></img>
					</g:Button>
				</div>
				<div class="lower">
					<g:Button styleName="store-select-mode" ui:field="storeSelectMode">
						<img src="images/Select_Enabled.svg"></img>
					</g:Button>
					<g:Button styleName="store" ui:field="store">
						<img src="images/Store_Enabled.svg"></img>
					</g:Button>
				</div>
			</div>

			<nl:BankUI></nl:BankUI>

			<div class="load-area">
				<div class="load-and-select-buttons">
					<div class="select-buttons">
						<g:Label styleName="number-of-presets" ui:field="numPresets">[64]</g:Label>
						<g:Button styleName="previous-preset" ui:field="previousPreset">
							<img src="images/Preset_Up_Enabled.svg"></img>
						</g:Button>
						<g:Button styleName="next-preset" ui:field="nextPreset">
							<img src="images/Preset_Down_Enabled.svg"></img>
						</g:Button>
					</div>
					<g:Button styleName="load-preset" ui:field="loadPreset">
						<img src="images/Load_Enabled.svg"></img>
					</g:Button>
				</div>
			</div>
		</div>

		<g:Label styleName="loaded-preset-number" ui:field="loadedPresetNumber"></g:Label>
		<g:Button styleName="direct-load" ui:field="directLoad">
			<img src="images/Dir-Load_Enabled.svg" class="large"></img>
			<img src="images/Link_Active.svg" class="small"></img>
		</g:Button>

	</g:HTMLPanel>
</ui:UiBinder>