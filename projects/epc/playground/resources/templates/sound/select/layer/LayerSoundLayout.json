{
  "layouts": {
    "Layer-Sound-Select-Layout": {
      "Selector": {
        "UIFocus": "Sound",
        "UIMode": "Select",
        "UIDetail": "Init"
      },
      "Conditions": [
        "isLayerSound"
      ],
      "Controls": {
        "Header": {
          "Class": "InvertedLabel",
          "Position": "0, 0",
          "Events": "EditBufferTypeText => Text[Text]"
        },
        "Name": {
          "Class": "WideHighlightLabel",
          "Position": "64, 0",
          "Events": "CurrentVoiceGroupLabel => Text[Text]"
        },
        "ButtonA": {
          "Class": "Button",
          "Position": "BUTTON_A",
          "Events": "SelectVGButtonText => Text[Text]"
        },
        "ButtonB": {
          "Class": "Button",
          "Position": "BUTTON_B",
          "Events": "SoundParamsButtonText => Text[Text]"
        },
        "ButtonC": {
          "Class": "Button",
          "Position": "BUTTON_C",
          "Init": {
            "Text[Text]": "Voices.."
          },
          "Events": "SoundVoicesButtonText => Text[Text]"
        },
        "ButtonD": {
          "Class": "Button",
          "Position": "BUTTON_D",
          "Init": {
            "Text[Text]": "Master.."
          },
          "Events": "SoundMasterButtonText => Text[Text]"
        },
        "Upper": {
          "Class": "LayerVoiceGroupI",
          "Position": "57, 16",
          "Init": {
            "VoiceGroupIdentifier[Text]": "I"
          },
          "Events": "isCurrentVGI => ANY[Highlight], VGIMasterTuneText => Unison[Text], VGIMasterVolumeText => Master[Text], VGIMuted => MuteIndication[Text]"
        },
        "Lower": {
          "Class": "LayerVoiceGroupII",
          "Position": "57, 32",
          "Init": {
            "VoiceGroupIdentifier[Text]": "II",
            "Unison[Text]": "",
            "Master[Text]": ""
          },
          "Events": "isCurrentVGII => ANY[Highlight], VGIIMasterTuneText => Unison[Text], VGIIMasterVolumeText => Master[Text], VGIIMuted => MuteIndication[Text]"
        },
        "FBBoth": {
          "Class": "LayerFBImageLarge",
          "Position": "29, 19",
          "Init": {
            "Image[ImagePath]": "Layer_FB_C.png",
            "ANY[Visibility]": "false"
          },
          "Events": "LayerFBIToIIAndIIToI => ANY[Visibility]"
        },
        "FBIToIIOnly": {
          "Class": "LayerFBImageLarge",
          "Position": "34, 24",
          "Init": {
            "Image[ImagePath]": "Layer_FB_B.png",
            "ANY[Visibility]": "false"
          },
          "Events": "LayerFBIToIIOnly => ANY[Visibility]"
        },
        "FBIIToIOnly": {
          "Class": "LayerFBImageLarge",
          "Position": "34, 24",
          "Init": {
            "Image[ImagePath]": "Layer_FB_A.png",
            "ANY[Visibility]": "false"
          },
          "Events": "LayerFBIIToIOnly => ANY[Visibility]"
        },
        "FXIToFXII": {
          "Class": "FXITOFXII",
          "Position": "202, 23",
          "Init": {
            "FXITOFXII_image[ImagePath]": "Layer_FX_B.png"
          },
          "Events": "ToFXIIOver0 => ANY[Visibility]"
        },
        "FXIIToFXI": {
          "Class": "FXITOFXII",
          "Position": "202, 21",
          "Init": {
            "FXITOFXII_image[ImagePath]": "Layer_FX_A.png"
          },
          "Events": "ToFXIOver0 => ANY[Visibility]"
        }
      },
      "EventSinks": {
        "BUTTON_A": "ToggleVoiceGroup",
        "BUTTON_B": "OpenPartScreen",
        "BUTTON_C": "SwitchToVoicesDetail",
        "BUTTON_D": "OpenMasterParameter",
        "BUTTON_INC": "LayerMuteInc",
        "BUTTON_DEC": "LayerMuteDec",
        "ROTARY_PLUS": "LayerMuteInc",
        "ROTARY_MINUS": "LayerMuteDec"
      }
    }
  }
}
