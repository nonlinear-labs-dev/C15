{
  "layouts": {
    "Modulateable-Parameter-MC-Select-Layout": {
      "Selector": {
        "UIFocus": "Parameters",
        "UIMode": "Select",
        "UIDetail": "ButtonB"
      },
      "Conditions": [
        "isParameterModulateable"
      ],
      "Controls": {
        "ButtonA": {
          "Class": "Button",
          "Position": "3, 51"
        },
        "ButtonB": {
          "Class": "HighlightButton",
          "Position": "67, 51",
          "Init": {
            "Text[Text]": "MC Sel"
          }
        },
        "ButtonC": {
          "Class": "Button",
          "Position": "131, 51"
        },
        "MCSelection-Label": {
          "Class": "Label",
          "Position": "67, 35",
          "Events": "MacroControlSymbol => Text[Text]"
        },
        "GroupLabel": {
          "Class": "InvertedLabel",
          "Position": "0, 0",
          "Events": "ParameterGroupName => Text[Text]"
        },
        "Slider": {
          "Class": "ParameterSlider",
          "Position": "77,  24 ",
          "Events": "SliderRange => Slider[Range], IsBipolar => CenterMark[Visibility]"
        },
        "ParameterName": {
          "Class": "ParameterNameLabel",
          "Position": "77, 10",
          "Events": "ParameterNameWithStateSuffix => Label[Text]"
        },
        "ModulationCarousel": {
          "Class": "ModulationCarousel",
          "Position": "195, 0"
        }
      },
      "EventSinks": {
        "BUTTON_INC": "IncMCSel",
        "BUTTON_DEC": "DecMCSel",
        "BUTTON_B": "SwitchToInitDetail"
      }
    }
  }
}