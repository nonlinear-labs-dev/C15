{
  "layouts": {
    "Unmodulateable-Parameter-Select-Layout": {
      "Selector": {
        "UIFocus": "Parameters",
        "UIMode": "Select",
        "UIDetail": "Init"
      },
      "Conditions": [
        "isParameterUnmodulateable"
      ],
      "Controls": {
        "ButtonA": {
          "Class": "Button",
          "Position": "3, 51"
        },
        "ButtonB": {
          "Class": "Button",
          "Position": "67, 51"
        },
        "ButtonC": {
          "Class": "Button",
          "Position": "131, 51"
        },
        "GroupLabel": {
          "Class": "InvertedLabel",
          "Position": "0, 0",
          "Events": "ParameterGroupName => Text[Text]"
        },
        "Slider": {
          "Class": "ParameterSlider",
          "Position": "77,  24 ",
          "Events": "SliderRange => Slider[Range], IsBipolar => CenterMark[Visibility]"
        },
        "ParameterName": {
          "Class": "ParameterNameLabel",
          "Position": "77, 10",
          "Events": "ParameterNameWithStateSuffix => Label[Text]"
        },
        "ParameterValue": {
          "Class": "ParameterValueLabel",
          "Position": "77, 35",
          "Events": "ParameterDisplayString => Label[Text]"
        },
        "ParameterCarousel": {
          "Class": "ParameterCarousel",
          "Position": "195, 0"
        }
      },
      "EventSinks": {
        "BUTTON_INC+": "IncParam",
        "BUTTON_DEC+": "DecParam",
        "BUTTON_D": "IncParamSelection"
      }
    }
  }
}