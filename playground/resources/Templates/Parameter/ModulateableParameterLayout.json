{
  "layouts": {
    "Modulateable-Parameter-Select-Layout": {
      "Selector": {
        "UIFocus": "Parameters",
        "UIMode": "Select",
        "UIDetail": "Init"
      },
      "Conditions": [
        "isParameterModulateable"
      ],
      "Controls": {
        "ButtonAEmpty": {
          "Class": "Button",
          "Position": "3, 51",
          "Visibility": [
            "!MacroControlAsignment"
          ]
        },
        "ButtonA": {
          "Class": "Button",
          "Position": "3, 51",
          "Init": {
            "Text[Text]": "MC Pos"
          },
          "Visibility": [
            "MacroControlAsignment",
            "!MCPositionChanged"
          ]
        },
        "ButtonAChanged": {
          "Class": "Button",
          "Position": "3, 51",
          "Init": {
            "Text[Text]": "MC Pos*"
          },
          "Visibility": [
            "MCPositionChanged"
          ]
        },
        "ButtonB": {
          "Class": "Button",
          "Position": "BUTTON_B",
          "Init": {
            "Text[Text]": "MC Sel"
          },
          "Visibility": [
            "!MCSelectionChanged"
          ]
        },
        "ButtonBChanged": {
          "Class": "Button",
          "Position": "BUTTON_B",
          "Init": {
            "Text[Text]": "MC Sel*"
          },
          "Visibility": [
            "MCSelectionChanged"
          ]
        },
        "ButtonCEmpty": {
          "Class": "Button",
          "Position": "BUTTON_C",
          "Init": {
            "Text[Text]": ""
          },
          "Visibility": [
            "!MacroControlAsignment"
          ]
        },
        "ButtonC": {
          "Class": "Button",
          "Position": "BUTTON_C",
          "Init": {
            "Text[Text]": "MC Amt"
          },
          "Visibility": [
            "!MCAmountChanged"
          ]
        },
        "ButtonCChanged": {
          "Class": "Button",
          "Position": "BUTTON_C",
          "Init": {
            "Text[Text]": "MC Amt*"
          },
          "Visibility": [
            "MCAmountChanged"
          ]
        },
        "GroupLabel": {
          "Class": "InvertedLabel",
          "Position": "0, 0",
          "Events": "ParameterGroupName => Text[Text]"
        },
        "Slider": {
          "Class": "ParameterSlider",
          "Position": "77,  24 ",
          "Events": "SliderRange => Slider[Range], IsBipolar => CenterMark[Visibility]"
        },
        "ParameterName": {
          "Class": "ParameterNameLabel",
          "Position": "77, 10",
          "Events": "ParameterNameWithStateSuffix => Label[Text]"
        },
        "ParameterValue": {
          "Class": "ParameterValueLabel",
          "Position": "77, 35",
          "Events": "ParameterDisplayString => Label[Text]"
        },
        "ParameterCarousel": {
          "Class": "ParameterCarousel",
          "Position": "195, 0"
        },
        "ModulationRange": {
          "Class": "ModRangeSlider",
          "Position": "77, 22",
          "Events": "MCModRange => UpperBar[Range], MCModRange => LowerBar[Range]"
        },
        "AssignedMCLabel": {
          "Class": "SmallLabel",
          "Position": "40, 21",
          "Events": "MacroControlSymbol => Text[Text]",
          "Visibility": "MacroControlAsignment"
        },
        "Dots": {
          "Class": "Dots",
          "Position": "60, 17",
          "Init": {
            "Text[Text]": "..."
          },
          "Visibility": "MacroControlAsignment"
        }
      },
      "EventSinks": {
        "BUTTON_INC": "IncParam",
        "BUTTON_DEC": "DecParam",
        "BUTTON_A": "SwitchToButtonADetail",
        "BUTTON_B": "SwitchToButtonBDetail",
        "BUTTON_C": "SwitchToButtonCDetail",
        "BUTTON_D": "SwitchToButtonDDetail"
      }
    }
  }
}