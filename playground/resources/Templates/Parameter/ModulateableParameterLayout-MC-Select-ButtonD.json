{
  "layouts": {
    "Modulateable-Parameter-MC-Select-Layout": {
      "Selector": {
        "UIFocus": "Parameters",
        "UIMode": "Select",
        "UIDetail": "ButtonD"
      },
      "Conditions": [
        "isParameterModulateable",
        "hasMcSelected"
      ],
      "Controls": {
        "ButtonA": {
          "Class": "Button",
          "Position": "3, 51",
          "Events": "MCPositionButtonText => Text[Text]"
        },
        "ButtonB": {
          "Class": "Button",
          "Position": "67, 51",
          "Events": "MCSelectButtonText => Text[Text]"
        },
        "ButtonC": {
          "Class": "Button",
          "Position": "131, 51",
          "Events": "MCAmountButtonText => Text[Text]"
        },
        "MCSelection-Label": {
          "Class": "MCSelectionLabel",
          "Position": "40, 21",
          "Events": "MacroControlSymbol => Text[Text]"
        },
        "Dots": {
          "Class": "Label",
          "Position": "38, 17",
          "Init": {
            "Text[Text]": "....."
          },
          "Visbility": "MacroControlAsignment"
        },
        "ParameterValue": {
          "Class": "ParameterValueLabel",
          "Position": "77, 35",
          "Events": "ParameterDisplayString => Label[Text]"
        },
        "GroupLabel": {
          "Class": "InvertedLabel",
          "Position": "0, 0",
          "Events": "ParameterGroupName => Text[Text]"
        },
        "Slider": {
          "Class": "ParameterSlider",
          "Position": "77,  24 ",
          "Events": "SliderRange => Slider[Range], IsBipolar => CenterMark[Visibility]"
        },
        "ParameterName": {
          "Class": "ParameterNameLabel",
          "Position": "77, 10",
          "Events": "ParameterNameWithStateSuffix => Label[Text]"
        },
        "ModulationRange": {
          "Class": "ModRangeSlider",
          "Position": "77, 22",
          "Events": "MCModRange => UpperBar[Range], MCModRange => LowerBar[Range]"
        },
        "ModulationCarousel": {
          "Class": "ModulationCarousel",
          "Position": "195, 0"
        }
      },
      "EventSinks": {
        "BUTTON_INC": "IncModulationCarousel",
        "BUTTON_DEC": "DecModulationCarousel",
        "BUTTON_A": "SwitchToButtonADetail",
        "BUTTON_B": "SwitchToButtonBDetail",
        "BUTTON_C": "SwitchToButtonCDetail",
        "BUTTON_D": "IncButtonMenu"
      }
    }
  }
}