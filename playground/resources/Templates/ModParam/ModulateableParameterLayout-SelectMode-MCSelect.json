{
  "layouts": {
    "ModulateableParameter-SelectMode-MCSelect": {
      "Selector": {
        "UIFocus": "Parameters",
        "UIMode": "Select",
        "UIDetail": "MCSelect"
      },
      "Conditions": [
        "isParameterModulateable"
      ],
      "Controls": {
        "GroupHeader": {
          "Class": "ParameterGroupName",
          "Position": "0,0",
          "Events": "ParameterGroupName => Text[Text]"
        },
        "Slider": {
          "Class": "BigSlider",
          "Position": "77,24",
          "Events": "IsBipolar => CenterMark[Visibility], SliderRange => Slider[Range]"
        },
        "ParameterName": {
          "Class": "ParameterNameLabel",
          "Position": "72,8",
          "Events": "ParameterName => Text[Text]"
        },
        "ButtonA-Empty": {
          "Class": "EmptyButton",
          "Position": "0,52"
        },
        "MCName": {
          "Class": "MCName",
          "Position": "72, 35",
          "Events": "MacroControlSymbol => Text[Text]"
        },
        "ButtonB-MCSel": {
          "Class": "McSelButton",
          "Position": "64,52"
        },
        "ButtonD-Empty": {
          "Class": "EmptyButton",
          "Position": "192,52"
        },
        "ButtonA-MCPos": {
          "Class": "EmptyButton",
          "Position": "0,52"
        },
        "MCPosSlider": {
          "Class": "MCPosSlider",
          "Position": "5,25",
          "Events": "MacroControlPosition => Circle[Range]"
        },
        "MCAmt": {
          "Class": "MCAmtLabel",
          "Position": "128, 35",
          "Events": "MacroControlAmount => Text[Text], MacroControlAsignment => Text[Visibility]"
        },
        "ButtonC-MCAmt": {
          "Class": "McAmtButton",
          "Position": "128,52",
          "Events": "MacroControlAsignment => Text[Visibility]"
        },
        "MCMenu": {
          "Class": "ModulationCarousel",
          "Position": "194,2"
        },
        "LockIndicator": {
          "Class": "LockIndicator1Letter",
          "Position": "65,1",
          "Events": "LockStatus => Text[Visibility]"
        }
      },
      "EventSinks": {
        "BUTTON_INC": "IncMCSel",
        "BUTTON_DEC": "DecMCSel",
        "ROTARY_PLUS": "IncMCSel",
        "ROTARY_MINUS": "DecMCSel",
        "BUTTON_EDIT": "SwitchToEditMode",
        "BUTTON_B": "SwitchToInitDetail",
        "BUTTON_C": "SwitchToMCAmtDetail",
        "BUTTON_D": "SwitchToMCModRangeDetail"
      }
    }
  }
}
